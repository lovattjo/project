<%- include('partials/header'); %> 
<form action="/register" method="post">
	<div class="form-group">
	  <label for="email">Email address</label>
	  <input type="email" class="form-control" id="email" aria-describedby="emailHelp" name="email" placeholder="Email">
	  <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
	</div>
	<div class="form-group">
	  <label for="password">Password</label>
	  <input type="password" class="form-control" id="password" name="password" placeholder="Password">
	</div>
	<div class="form-group">
		<label for="password">Password Confirm</label>
		<input type="password" class="form-control" id="password" name="passwordConfirm" placeholder="Confirm Password">
	  </div>
	<button type="submit" class="btn btn-primary">Register</button>
  </form>
<%- include('partials/footer'); %>